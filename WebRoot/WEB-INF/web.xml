<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <listener>
    <listener-class>com.wsj.Web.Listener.ApplicationListener</listener-class>
  </listener>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.LoginSer</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>productDetailSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.productDetailSer</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>registerSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.registerSer</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>addProductSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.addProductSer</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>deleteOrder</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.deleteOrder</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>exitSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.exitSer</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>modifyUserSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.modifyUserSer</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>orderSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.orderSer</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>removeAllProductSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.removeAllProductSer</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>saveOrder</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.saveOrder</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>removeProductSer</servlet-name>
    <servlet-class>com.wsj.Web.Servlet.removeProductSer</servlet-class>
  </servlet>

  <filter>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>com.wsj.Web.Filter.EncodingFilter</filter-class>
  </filter>
  <filter>
    <filter-name>LoginFilter</filter-name>
    <filter-class>com.wsj.Web.Filter.LoginFilter</filter-class>
  </filter>


  <servlet-mapping>
    <servlet-name>LoginSer</servlet-name>
    <url-pattern>/LoginSer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>productDetailSer</servlet-name>
    <url-pattern>/productDetailSer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>registerSer</servlet-name>
    <url-pattern>/registerSer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addProductSer</servlet-name>
    <url-pattern>/user/addProductSer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deleteOrder</servlet-name>
    <url-pattern>/user/deleteOrder</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>exitSer</servlet-name>
    <url-pattern>/exitSer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>modifyUserSer</servlet-name>
    <url-pattern>/user/modifyUserSer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>orderSer</servlet-name>
    <url-pattern>/user/orderSer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>removeAllProductSer</servlet-name>
    <url-pattern>/user/removeAllProductSer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>saveOrder</servlet-name>
    <url-pattern>/user/saveOrder</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>removeProductSer</servlet-name>
    <url-pattern>/user/removeProductSer</url-pattern>
  </servlet-mapping>
  
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/user/*</url-pattern>
  </filter-mapping>
  
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
